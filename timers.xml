<settings>
    <global>
        <EnableRecast>true</EnableRecast>
        <EnableBuffs>true</EnableBuffs>
        <GraphicalMode>true</GraphicalMode>
        <ExtendText>true</ExtendText>
        <TextColoring>false</TextColoring>
        <SlimMode>false</SlimMode>
        <ShowTenths>true</ShowTenths>
        <!--One of: Alphabetical, Creation, Duration-->
        <SortType>Creation</SortType>
        <Show1HourName>true</Show1HourName>
        <HighRed>255</HighRed>
        <HighGreen>0</HighGreen>
        <HighBlue>0</HighBlue>
        <MedPercent>0.5</MedPercent>
        <MedRed>255</MedRed>
        <MedGreen>160</MedGreen>
        <MedBlue>0</MedBlue>
        <LowPercent>0.25</LowPercent>
        <LowRed>0</LowRed>
        <LowGreen>255</LowGreen>
        <LowBlue>0</LowBlue>
        <TextA>255</TextA>
        <TextR>255</TextR>
        <TextG>255</TextG>
        <TextB>255</TextB>
        <RecastLowToHigh>true</RecastLowToHigh>
        <BuffsLowToHigh>false</BuffsLowToHigh>
        <CustomLowToHigh>false</CustomLowToHigh>
        <BGA>128</BGA>
        <BGR>0</BGR>
        <BGG>0</BGG>
        <BGB>0</BGB>
        <RecastFlashDuration>2</RecastFlashDuration>
        <BuffsFlashDuration>2</BuffsFlashDuration>
        <CustomFlashDuration>2</CustomFlashDuration>
        <FlashR>255</FlashR>
        <FlashG>255</FlashG>
        <FlashB>0</FlashB>
        <FontName>Arial</FontName>
        <FontSize>8.5</FontSize>
        <FontBold>true</FontBold>
        <BarSpacing>0</BarSpacing>
        <RecastTimerLimit>10</RecastTimerLimit>
        <BuffsTimerLimit>10</BuffsTimerLimit>
        <CustomTimerLimit>10</CustomTimerLimit>
        <RecastX>1080</RecastX>
        <RecastY>60</RecastY>
        <BuffsX>1260</BuffsX>
        <BuffsY>60</BuffsY>
        <CustomX>900</CustomX>
        <CustomY>60</CustomY>
        <!--If true, only items on the recast/buff lists will be displayed-->
        <WhitelistMode>false</WhitelistMode>
        <!--List of ability names to display recasts for if in whitelistMode and to exclude otherwise-->
        <AbilityRecastFilter />
        <!--List of spell names to display recasts for if in whitelistMode and to exclude otherwise-->
        <SpellRecastFilter>Protect*|Shell*|Bar*|August|Areuhat|Amchuchu|Kupipi|Koru-Moru|Shantotto*|Joachim|Star Sibyl|Moogle|Sakura|Kuyin Hathdenna|Qultada|Ulmia|Karaha-Baruha|Kupofried|Brygid|Selh&apos;teus|Yoran-Oran*|King of Hearts|Iroha*|Cherukiki|Arciela*</SpellRecastFilter>
        <!--List of ability names to display buffs for if in whitelistMode and to exclude otherwise-->
        <AbilityBuffFilter>Ignis|Gelus|Flabra|Tellus|Sulpor|Unda|Lux|Tenebrae|Light Arts|Dark Arts|Afflatus*|Sublimation|Bust|Composure</AbilityBuffFilter>
        <!--List of spell names to display buffs for if in whitelistMode and to exclude otherwise-->
        <!--If true will show a font outline-->
        <Stroke>true</Stroke>
        <!--List of spell names to display buffs for if in whitelistMode and to exclude otherwise-->
        <SpellBuffFilter>Protect*|Shell*|Bar*|Aquaveil|Blink|Stoneskin|Reraise*|Utsusemi*|Occultation|Barrier Tusk|Diamond Hide</SpellBuffFilter>
    </global>
</settings>
